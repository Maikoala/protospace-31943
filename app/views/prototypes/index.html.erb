<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting"> 
        こんにちは、
        <span><%= link_to "#{current_user.name}さん", user_path(current_user.id), class: :greeting__link%></span>
      </div> 
    <% end %>
    <div class="card__wrapper">
      <%= render partial: 'prototype', collection: @prototypes %>
    </div>
  </div>
</main>
